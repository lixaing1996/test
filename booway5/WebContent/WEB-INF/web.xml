    <servlet-name>userXml</servlet-name>
    <url-pattern>/userXml</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>userXmlAdd</servlet-name>
    <servlet-class>servlet.AddXmlServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>userXmlAdd</servlet-name>
    <url-pattern>/userXmlAdd</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>userXmlDelete</servlet-name>
    <servlet-class>servlet.DeleteXmlServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>userXmlDelete</servlet-name>
    <url-pattern>/userXmlDelete</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>userXmlUpdate</servlet-name>
    <servlet-class>servlet.UpdateXmlServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>userXmlUpdate</servlet-name>
    <url-pattern>/userXmlUpdate</url-pattern>
  </servlet-mapping>
